{
  "expo": {
    "name": "Boon-Markt",
    "slug": "Boon-Markt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  },
  "api":{
    "setting":"https://cllqtv-bn-dev-api.azurewebsites.net/api/inventory/settings",
    "appBook": "https://cllqtv-bn-dev-api.azurewebsites.net/api/inventory/locations/SelfScan/settings",
    "language": "https://stcllqtvbndev.blob.core.windows.net/translation/consumer/en/translations.json"
  }
}
